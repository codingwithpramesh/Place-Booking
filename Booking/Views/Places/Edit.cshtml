@model Places

@{
    ViewData["Title"] = "Edit Countries";
}
<form asp-action="Edit" method="post" enctype="multipart/form-data">
    <div class="row">
        <div class="col-md-8 offset-2">
            <div class="form-group">
                <label asp-for="HostId">Country Code</label>
                <input asp-for="HostId" class="form-control" />
                <span asp-validation-for="HostId"></span>
            </div>

            <div class="form-group">
                <label asp-for="Address"> Address</label>
                <input asp-for="Address" class="form-control" />
                <span asp-validation-for="Address"></span>
            </div>

            <div class="form-group">
                <label asp-for="CityId">Country </label>
                <input asp-for="CityId" class="form-control" />
                <span asp-validation-for="CityId"></span>
            </div>

            <div class="form-group">
                <label asp-for="Image">Country </label>
                <input type="file" asp-for="Image" name="file" value="abc()" class="form-control" />
                <span asp-validation-for="Image"></span>
            </div>


            <div class="form-group">
                <button type="submit" asp-action="Edit" class="btn btn-primary">Edit</button>
            </div>

        @*</div><a href="d:\dot net projects\booking\booking\views\places\edit.cshtml">d:\dot net projects\booking\booking\views\places\edit.cshtml</a>*@
    </div>
        <script type="text/javascript">
            function abc() {
                var str = "";
                document.getElementById("FileName").value = str;
            }
        </script>
</form>
