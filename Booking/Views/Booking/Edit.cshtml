@model Booked

@{
    ViewData["Title"] = "Edit Countries";
}
<form asp-action="Edit" method="post">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="row">
        <div class="col-md-8 offset-2">
            <div class="form-group">
                <label asp-for="userID">User Id</label>
                <input asp-for="userID" class="form-control" />
                <span asp-validation-for="userID"></span>
            </div>

            <div class="form-group">
                <label asp-for="placeId">Place Id</label>
                <input asp-for="placeId" class="form-control" />
                <span asp-validation-for="placeId"></span>
            </div>

            <div class="form-group">
                <label asp-for="StartDate">Start Date</label>
                <input asp-for="StartDate" class="form-control" />
                <span asp-validation-for="StartDate"></span>
            </div>

            <div class="form-group">
                <label asp-for="EndDate">End Date:</label>
                <input asp-for="EndDate" class="form-control" />
                <span asp-validation-for="EndDate"></span>
            </div>

          

            <div class="form-group">
                <label asp-for="PricePerNight"> Price Per Night </label>
                <input asp-for="PricePerNight" class="form-control" />
                <span asp-validation-for="PricePerNight"></span>
            </div>

            <div class="form-group">
                <label asp-for="Num_night">Number Night </label>
                <input asp-for="Num_night" class="form-control" />
                <span asp-validation-for="Num_night"></span>
            </div>

            <div class="form-group">
                <button type="submit" asp-action="Edit" class="btn btn-primary">Edit</button>
            </div>
        </div>
    </div>

</form>
